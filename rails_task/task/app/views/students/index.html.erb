<table class = "table table-striped table-bordered table-hover">
    <thead class = "thead-dark">
      <tr>
        <th>Name</th>
        <th>Age</th>
        <th>Phone</th>
        <th>Email</th>
      </tr>
    </thead>

    <tbody>
      <% @students.each do |student| %>
          <tr>
            <td><%= link_to student.first_name + " " + student.last_name, student %></td>
            <td><%= student.age %></td>
            <td><%= student.phone %></td>
            <td><%= student.email %></td>
          </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to 'New Student', new_student_path, class: "btn btn-dark"%>
